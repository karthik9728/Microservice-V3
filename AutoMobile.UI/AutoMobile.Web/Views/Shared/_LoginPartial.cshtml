@using AutoMobile.Domain.ApplicationConstants;
@inject Microsoft.AspNetCore.Http.IHttpContextAccessor httpAcc;
<ul class="navbar-nav">
    @if (httpAcc.HttpContext.Session.GetString(ApplicationConstant.SessionToken) != null &&
    httpAcc.HttpContext.Session.GetString(ApplicationConstant.SessionToken).Length > 0)
    {
        <li class="nav-item">
            <a class="nav-link" asp-controller="Auth" asp-action="Logout">Logout</a>
        </li>
    }
    else
    {
        <li class="nav-item">
            <a class="nav-link" asp-controller="Auth" asp-action="Login">Login</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" asp-controller="Auth" asp-action="Register">Register</a>
        </li>
    }
</ul>